{
  "build": {
  },
  "bridge": {
    "worker_attribute_set": {
      "attributes": [
        "SimulatedPlayer"
      ]
    },
    "entity_interest": {
      "range_entity_interest": {
        "radius": 60
      }
    },
    "component_delivery": {
      "default": "RELIABLE_ORDERED",
      "checkoutAllInitially": true
    }
  },
  "external": {
    "default": {
      "run_type": "EXECUTABLE",
      "windows": {
        "command": "build/worker/SimulatedPlayer@Windows/SimulatedPlayer@Windows.exe",
        "arguments": [
          "+workerType",
          "SimulatedPlayer",
          "+infraServicesUrl",
          "http://127.0.0.1:21000",
          "+projectName",
          "${IMPROBABLE_PROJECT_NAME}",
          "-logfile",
          "../../logs/external-default-simulatedplayer.log"
        ]
      },
      "macos": {
        "command": "open",
        "arguments": [
          "./build/worker/UnityGameLogic@Mac/SimulatedPlayer@Mac.app",
          "--args",
          "+assemblyName",
          "local_assembly",
          "+workerType",
          "SimulatedPlayer",
          "+infraServicesUrl",
          "http://127.0.0.1:21000",
          "+projectName",
          "${IMPROBABLE_PROJECT_NAME}",
          "-logfile",
          "../../logs/external-default-simulatedplayer.log"
        ]
      }
    }
  }
}
