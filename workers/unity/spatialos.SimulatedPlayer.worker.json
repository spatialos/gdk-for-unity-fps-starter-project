{
  "build": {
  },
  "bridge": {
    "worker_attribute_set": {
      "attributes": [
        "SimulatedPlayer"
      ]
    },
    "entity_interest": {
      "range_entity_interest": {
        "radius": 60
      }
    },
    "streaming_query": [
      {
        "global_component_streaming_query": {
          "component_name": "improbable.gdk.safezone.SafeZone"
        },
        "components_streamed": {
          "specific_components": {
            "components": [
              "improbable.Position",
              "improbable.Metadata",
              "improbable.gdk.safezone.SafeZone"
            ]
          }
        }
      }
    ],
    "component_delivery": {
      "default": "RELIABLE_ORDERED",
      "checkoutAllInitially": true
    }
  },
  "external": {
    "default": {
      "run_type": "EXECUTABLE",
      "windows": {
        "command": "build/worker/SimulatedPlayer@Windows/SimulatedPlayer@Windows.exe",
        "arguments": [
          "+workerType",
          "SimulatedPlayer",
          "+infraServicesUrl",
          "http://127.0.0.1:21000",
          "+projectName",
          "${IMPROBABLE_PROJECT_NAME}",
          "-logfile",
          "../../logs/external-default-unityclient.log"
        ]
      }
    }
  }
}
